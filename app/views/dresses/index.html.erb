 <div class="container">
  <div class="row">
     <% @dresses.each do |dress| %>
     <div class="col-md-3">
     <%= link_to dress_path(dress.id) do %>
       <%= attachment_image_tag dress, :image, format: 'jpeg', size: "150x150" %>
     <% end %><br>

     <%= dress.genre.name %><br>
     <%= dress.get_season_name %><br>
     <%= dress.size %> cm<br>
     <%= dress.explanation %><br>
</div>
     <% end %>
</div></div>